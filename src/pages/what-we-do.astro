---
import Layout from '@layouts/Layout.astro';
import Header from '@components/Header.astro';
import Footer from '@components/Footer.astro';
---

<Layout title='Astro'>
	<Header />
	<div class='bg-brand-dark-blue relative'>
		<img
			src='/img/bg-hero-what-we-do.webp'
			class='absolute inset-x-0 top-0 z-20 block h-[155px] w-full rounded-b-full object-cover [clip-path:ellipse(50%_120%_at_50%_-20%)] sm:hidden'
			alt=''
		/>
		<div
			class='bg-brand-dark-blue/70 absolute inset-x-0 top-0 z-20 block h-[155px] w-full rounded-b-full object-cover [clip-path:ellipse(50%_120%_at_50%_-20%)] sm:hidden'>
		</div>
		<div class='container'>
			<div class='relative pt-52 pb-16 sm:pt-80 md:pb-16 lg:pt-36 lg:pb-32 xl:pt-44 xl:pt-52 xl:pb-40'>
				<img
					src='/img/bg-hero-what-we-do.webp'
					class='absolute top-0 right-0 z-20 h-52 w-full rounded-b-full object-cover hidden sm:block sm:h-70 sm:w-3/4 sm:-translate-x-1/6 md:w-2/3 md:-translate-x-1/4 lg:right-8 lg:h-140 lg:w-[45%] lg:translate-x-0 xl:right-6 xl:h-180 2xl:right-5 2xl:w-2/5'
					alt=''
				/>
				<div
					class='bg-brand-dark-blue/70 lg:bg-brand-dark-blue/50 absolute top-0 right-0 z-20 hidden h-52 w-full rounded-b-full object-cover sm:block sm:h-70 sm:w-3/4 sm:-translate-x-1/6 md:w-2/3 md:-translate-x-1/4 lg:right-8 lg:h-140 lg:w-[45%] lg:translate-x-0 xl:right-6 xl:h-180 2xl:right-5 2xl:w-2/5'>
				</div>
				<div class='text-center lg:w-2/5 lg:text-left xl:w-[45%]'>
					<h3 class='text-brand-gold pb-4 text-2xl font-medium lg:pb-1'>What we do</h3>
					<p class='text-lg leading-8 font-medium text-white lg:text-xl xl:text-2xl xl:leading-10'>
						We specialize in executive search and offer leadership consulting and industry intelligence to help
						organizations shaping the built environment lead with insight, confidence, and resilience.
					</p>
				</div>
			</div>
		</div>
	</div>

	<div class='bg-brand-dark-blue relative'>
		<div
			class='absolute inset-0 z-10 h-1/2 w-full bg-[linear-gradient(0deg,rgba(149,128,83,0)_0%,rgba(149,128,83,0.35)_40.87%,rgba(149,128,83,0.35)_61.06%,rgba(149,128,83,0)_100%)]'>
		</div>
		<div class='relative z-20 container'>
			<div
				class='flex flex-col justify-center gap-5 divide-white pb-20 text-center font-light text-white sm:pt-24 sm:pb-14 lg:flex-row lg:gap-0 lg:pt-48 lg:pb-28 lg:text-xl xl:divide-x xl:pt-60 xl:pb-32 2xl:pt-68'>
				<a
					href='/what-we-do#executive-search'
					class='hover:text-brand-gold btn underline lg:px-6 lg:no-underline'
					>Executive Search</a
				>
				<a
					href='/what-we-do#leadership-consulting'
					class='hover:text-brand-gold btn underline lg:px-6 lg:no-underline'
					>Leadership Consulting</a
				>
				<a
					href='/what-we-do#industry-intelligence'
					class='hover:text-brand-gold btn underline lg:px-6 lg:no-underline'
					>Industry Intelligence</a
				>
			</div>
		</div>
		<div class='relative z-30'>
			<div class='relative'>
				<div
					class='absolute top-0 left-1/2 h-[155px] w-full -translate-x-1/2 rounded-b-full bg-[linear-gradient(160.91deg,#3FD0CC_30.97%,#958053_82.71%)] object-cover object-bottom [clip-path:ellipse(50%_120%_at_50%_-20%)] sm:h-40 md:h-80 md:[clip-path:ellipse(53%_130%_at_50%_-45%)] lg:left-0 lg:h-100 lg:w-1/2 lg:translate-x-0 lg:[clip-path:none] xl:h-110 2xl:h-130'>
					<div
						class='relative top-8 left-0 z-40 w-full text-center text-white sm:w-4/5 md:w-2/3 lg:absolute lg:top-14 lg:w-full xl:top-20 2xl:top-24'>
						<div class='mx-auto hidden w-4/5 sm:w-1/2 md:w-2/5 lg:block lg:w-3/4 2xl:w-2/3'>
							<h2 class='pb-5 text-2xl font-medium sm:text-3xl md:text-4xl xl:pb-7'>Executive Search</h2>
							<p
								class='text-brand-dark-blue text-lg leading-8 font-light sm:text-xl lg:text-2xl xl:leading-9 2xl:text-3xl 2xl:leading-11'>
								We find senior leaders who bring more than just experience—they bring energy, vision, and the ability to
								make things happen
							</p>
						</div>
					</div>
				</div>
			</div>
